{% block html_head_container %}

  {% include 'html-header.twig' %}
  {% block head %}{% endblock %}
</head>{% endblock %}<body class="{{body_class}}" data-template="base.twig">
<div class="layout-main">
  <header class="header">
    <a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>
    {% block header %}
      <div class="wrapper">
        <h1 class="hdr-logo" role="banner">
          <a class="hdr-logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
        </h1>
        <nav class="nav-main" id="nav-main" role="navigation">
          {% include "menu.twig" with {'menu': menu.get_items} %}
        </nav>
        <!-- #nav -->
      </div>
    {% endblock %}
  </header>

  <section class="content-wrapper main max-width-container" id="content" role="main">
    <section class="max-width-container__content">
      {% if title %}
        <h1>{{title}}</h1>
      {% endif %}
      <div class="wrapper {{sidebar_class}}">
        {% block content %}
          Sorry, no content
        {% endblock %}
      </div>
      {% if sidebar %}
        <aside class="layout-sidebar">
          {{sidebar}}
        </aside>
      {% endif %}
    </section>
  </section>

  {% block footer %}
    <footer class="footer" id="footer">
      {% include 'footer.twig' %}
    </footer>
    {{ function('wp_footer') }}
  {% endblock %}
</div></body></html>
